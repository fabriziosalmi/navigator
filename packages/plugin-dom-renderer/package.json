{
  "name": "@navigator.menu/plugin-dom-renderer",
  "version": "1.0.0",
  "description": "DOM rendering and UI adaptation plugin for Navigator.menu",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "navigator",
    "dom",
    "renderer",
    "ui",
    "plugin"
  ],
  "author": "Navigator SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabriziosalmi/navigator.git",
    "directory": "packages/plugin-dom-renderer"
  },
  "peerDependencies": {
    "@navigator.menu/core": "workspace:*",
    "@navigator.menu/types": "workspace:*"
  },
  "devDependencies": {
    "@navigator.menu/core": "workspace:*",
    "@navigator.menu/types": "workspace:*",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^1.6.1",
    "happy-dom": "^20.0.10",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
