{
  "name": "@navigator.menu/plugin-keyboard",
  "version": "2.0.3",
  "description": "Keyboard navigation plugin for Navigator.Menu",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "test:ci": "vitest --run --config vitest.config.ci.ts --coverage --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "navigator",
    "keyboard",
    "plugin",
    "navigation",
    "accessibility"
  ],
  "author": "Navigator SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabriziosalmi/navigator.git",
    "directory": "packages/plugin-keyboard"
  },
  "peerDependencies": {
    "@navigator.menu/core": "workspace:*"
  },
  "devDependencies": {
    "@navigator.menu/core": "workspace:*",
    "@types/node": "^20.17.10",
    "@vitest/coverage-v8": "^1.6.1",
    "happy-dom": "^20.0.10",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@navigator.menu/types": "workspace:*"
  }
}
