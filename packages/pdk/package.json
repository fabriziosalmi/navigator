{
  "name": "@navigator.menu/pdk",
  "version": "3.0.0",
  "description": "Plugin Development Kit for Navigator - utilities, mocks, and types for building plugins",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "navigator",
    "plugin",
    "sdk",
    "development-kit"
  ],
  "author": "Navigator SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabriziosalmi/navigator.git",
    "directory": "packages/pdk"
  },
  "dependencies": {
    "@navigator.menu/types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
