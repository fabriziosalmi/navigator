{
  "name": "@navigator.menu/react",
  "version": "0.1.0",
  "description": "React integration for Navigator.Menu - BYOS (Bring Your Own State) v0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "navigator",
    "react",
    "hooks",
    "navigation",
    "ui"
  ],
  "author": "Navigator.Menu Team",
  "license": "MIT",
  "peerDependencies": {
    "@navigator.menu/core": "workspace:*",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@navigator.menu/core": "workspace:*",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@vitest/coverage-v8": "^1.6.1",
    "happy-dom": "^15.11.7",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^1.6.1"
  }
}
