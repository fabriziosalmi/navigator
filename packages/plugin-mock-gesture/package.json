{
  "name": "@navigator.menu/plugin-mock-gesture",
  "version": "3.0.0",
  "description": "Mock gesture plugin for demos and testing - auto-emits swipe events",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run --passWithNoTests",
    "test:coverage": "vitest --coverage --passWithNoTests"
  },
  "keywords": [
    "navigator",
    "plugin",
    "gesture",
    "mock",
    "demo",
    "testing"
  ],
  "author": "Navigator SDK Team",
  "license": "MIT",
  "peerDependencies": {
    "@navigator.menu/core": "workspace:*"
  },
  "devDependencies": {
    "@navigator.menu/core": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3",
    "vitest": "^2.0.0"
  }
}
